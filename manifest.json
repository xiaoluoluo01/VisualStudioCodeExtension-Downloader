{
  "manifest_version": 3,
  "name": "VSCode Extension Downloader",
  "version": "1.0",
  "description": "在VS Code插件市场页面添加下载按钮",
  "permissions": ["activeTab"],
  "content_scripts": [{
    "matches": ["https://marketplace.visualstudio.com/items*"],
    "js": ["content.js"]
  }],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}